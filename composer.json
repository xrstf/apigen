{
    "name": "apigen/apigen",
    "description": "API documentation generator for PHP 5.3+",
    "keywords": [
        "php",
        "documentation",
        "api"
    ],
    "authors": [
        {
            "name": "Jaroslav Hanslík",
            "homepage": "https://github.com/kukulich"
        },
        {
            "name": "Ondřej Nešpor",
            "homepage": "https://github.com/Andrewsville"
        },
        {
            "name": "David Grudl",
            "homepage": "https://github.com/dg"
        }
    ],
    "homepage": "http://www.apigen.org/",
    "license": "BSD-3-Clause",
    "repositories": [
        {
            "type": "pear",
            "url": "http://pear.kukulich.cz/"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "ext-tokenizer": "*",
        "ext-mbstring": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "nette/nette": "2.0.*",
        "dg/texy": "2.*",
        "andrewsville/php-token-reflection": "1.3.*",
        "pear-kukulich/FSHL": "2.*"
    },
    "autoload": {
        "psr-0": {
            "ApiGen": "."
        }
    },
    "config": {
        "vendor-dir": "libs"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.x-dev"
        }
    }
}
